{
  "name": "@bradrichardson/overturemaps",
  "version": "0.3.3",
  "description": "Unofficial JavaScript/TypeScript client for Overture Maps Foundation data - inspired by overturemaps-py",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external parquet-wasm,@duckdb/duckdb-wasm",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "overture",
    "overture-maps",
    "geospatial",
    "maps",
    "gis",
    "openstreetmap",
    "parquet",
    "geoparquet",
    "geojson",
    "gers"
  ],
  "author": "Brad Richardson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brad-richardson/overturemaps-js.git"
  },
  "bugs": {
    "url": "https://github.com/brad-richardson/overturemaps-js/issues"
  },
  "homepage": "https://github.com/brad-richardson/overturemaps-js#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loaders.gl/core": "^4.3.4",
    "@loaders.gl/wkt": "^4.3.4",
    "apache-arrow": "^21.1.0",
    "parquet-wasm": "^0.7.1"
  },
  "devDependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.10.0",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0"
  },
  "peerDependenciesMeta": {
    "@duckdb/duckdb-wasm": {
      "optional": true
    }
  },
  "module": "./dist/index.js"
}
