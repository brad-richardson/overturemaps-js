{
  "name": "@bradrichardson/overturemaps",
  "version": "0.3.0",
  "description": "Unofficial JavaScript/TypeScript client for Overture Maps Foundation data - inspired by overturemaps-py",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "overture",
    "overture-maps",
    "geospatial",
    "maps",
    "gis",
    "openstreetmap",
    "parquet",
    "geoparquet",
    "geojson",
    "gers"
  ],
  "author": "Brad Richardson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brad-richardson/overturemaps-js.git"
  },
  "bugs": {
    "url": "https://github.com/brad-richardson/overturemaps-js/issues"
  },
  "homepage": "https://github.com/brad-richardson/overturemaps-js#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "hyparquet": "^1.23.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.0"
  }
}
